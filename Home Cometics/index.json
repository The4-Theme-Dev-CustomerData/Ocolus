        "3aa8da3a-ab16-4b20-836a-c718f35dc0bd": {
          "type": "quote",
          "settings": {
            "content": "Amazing theme and top class support, as I’m a beginner, Luke helped me above and beyond and was more than patient move forward with your theme!",
            "image": "shopify:\/\/shop_images\/22_copy.png",
            "auth_name": "- Hindy",
            "rating": 5
          }
        },
        "61c05f7f-1c37-4f71-83fc-564698335972": {
          "type": "quote",
